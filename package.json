    "clsx": "^1.2.1",
    "deepmerge": "^4.3.1",
    "formik": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-tooltip": "^5.13.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.237",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
