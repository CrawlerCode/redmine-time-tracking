{
  "name": "redmine-time-tracking",
  "description": "Redmine Time Tracking",
  "version": "1.10.3",
  "author": {
    "name": "CrawlerCode",
    "email": "crawlercode@outlook.de"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/CrawlerCode/redmine-time-tracking"
  },
  "type": "module",
  "scripts": {
    "dev": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "pack": "node scripts/pack.js",
    "release": "gh release create v%npm_package_version% release/redmine-time-tracking-v%npm_package_version%.zip --notes-file release_notes.md --generate-notes",
    "publish": "pnpm run build && pnpm run pack && pnpm run release",
    "publish:beta": "pnpm run build && pnpm run pack && pnpm run release --prerelease --target beta",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tanstack/react-query": "^4.32.6",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "deepmerge": "^4.3.1",
    "flatpickr": "^4.6.13",
    "formik": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^3.10.13",
    "react-intl": "^6.4.4",
    "react-router-dom": "^6.14.2",
    "react-select": "^5.7.4",
    "react-tooltip": "^5.20.0",
    "tailwind-merge": "^1.14.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.243",
    "@types/node": "^20.4.8",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "@types/react-flatpickr": "^3.8.8",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react": "^4.0.4",
    "adm-zip": "^0.5.10",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
