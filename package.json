    "react-flatpickr": "^3.10.13",
    "react-intl": "^7.1.5",
    "react-router-dom": "^7.1.5",
    "react-select": "^5.10.0",
    "react-tooltip": "^5.28.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-shadow-fill": "^1.0.1",
    "tailwindcss-text-fill": "^0.2.0",
    "yup": "^1.6.1"
